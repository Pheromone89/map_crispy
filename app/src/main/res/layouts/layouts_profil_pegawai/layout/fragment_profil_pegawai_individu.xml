<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blueBasic"
    android:orientation="vertical"
    android:clickable="true">

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/i_profil_picture_individu"
        android:layout_marginBottom="10dp"/>

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/i_profil_data_pokok_individu"
        android:layout_marginBottom="10dp" />

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/i_profil_data_diklat_individu"
        android:layout_marginBottom="10dp" />

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/i_profil_data_pengembangan_individu"
        android:layout_marginBottom="10dp" />

</LinearLayout>